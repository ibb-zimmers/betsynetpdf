+	RectF		bbox( 0, 0, 0, 0);
+	g->MeasureString(wslabel.c_str(), -1, &font, pointF, &bbox);
+
+	if(this->dx_dpi > 0.0 && this->dy_dpi > 0.0)
+	{
+		bbox.Width = this->dx_dpi * zoom;
+		bbox.Height = this->dy_dpi * zoom;
+	}
