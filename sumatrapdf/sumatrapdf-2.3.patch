+RectF OverlayObject::GetRect(std::vector<PointF> points)
+{
+	float minX = 0, minY = 0, maxX = 0, maxY = 0;
+
+	int i;
+	PointF curPoint;
+	for(i = 0; i < points.size(); i++)
+	{
+		curPoint = points[i]; 
+		if(i == 0)
+		{
+			minX = maxX = curPoint.X;
+			minY = maxY = curPoint.Y;
+		}
+
+		if(curPoint.X < minX)
+			minX = curPoint.X;
+		if(curPoint.X > maxX)
+			maxX = curPoint.X;
+
+		if(curPoint.Y < minY)
+			minY = curPoint.Y;
+		if(curPoint.Y > maxY)
+			maxY = curPoint.Y;
+	}
+
+	return RectF(minX, minY, maxX - minX, maxY -  minY);
+}
