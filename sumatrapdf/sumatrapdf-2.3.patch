+	if((pointOnScreen.x + bbox.Width) < bounds.x || pointOnScreen.x > (bounds.x + bounds.dx))
+		return;
+
+	if((pointOnScreen.y + bbox.Height) < bounds.y || pointOnScreen.y > (bounds.y + bounds.dy))
+		return;
